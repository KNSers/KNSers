@{
    ViewBag.Title = "Index";
}

<head>
    <title>Rss Feed</title>

</head>
<body>
    <div id="bodyContainer" data-ng-app='RssApp'>
        <section id="content">
            <div class="container">
                <div class="row">
                    <div class="flip-container text-center col-md-12" data-ng-controller='RssEventController' ng-init="nextQuestion()">
                        <div class="back" ng-class="{flip: answered, correct: correctAnswer, incorrect:!correctAnswer}">
                            <p>
                                <table class="">
                                    <tr>
                                        <td><h2>Chương trình | Sự kiện</h2></td>
                                    </tr>
                                    <tr data-ng-repeat="option in options">
                                        <td>
                                            <a href="{{option.websiteLink}}">
                                                {{option.title}}
                                            </a>
                                        </td>
                                    </tr>
                                </table>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>


</body>



